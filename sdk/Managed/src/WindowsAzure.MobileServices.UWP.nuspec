<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>Anuracode.WindowsAzure.MobileServices.UWP</id>
    <version>1.3.2.6</version>
    <title>Windows Azure Mobile Services for universal windows apps</title>
    <authors>Anuracode</authors>
    <owners>Microsoft,azure-sdk</owners>
    <licenseUrl>http://go.microsoft.com/fwlink/?LinkId=286110</licenseUrl>
    <projectUrl>https://github.com/Azure/azure-mobile-services</projectUrl>
    <iconUrl>http://go.microsoft.com/fwlink/?LinkID=288890</iconUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <description>This client library enables client applications to connect to Windows Azure Mobile Services. Mobile Services allows you to develop an app with a scalable and secure backend hosted in Windows Azure. You can incorporate structured storage, user authentication and push notifications in minutes. For more information, check out https://www.windowsazure.com/mobile.</description>
    <summary>Windows Azure Mobile Services SDK with bug fixes, UWP and changed SQLite PCL library.</summary>
    <releaseNotes>https://github.com/Azure/azure-mobile-services/blob/master/CHANGELOG.md</releaseNotes>
    <language>en-US</language>
    <tags>microsoft windows azure mobile services</tags>
    <dependencies>
      <dependency id="Newtonsoft.Json" version="6.0.8" />
      <dependency id="Microsoft.Bcl" version="1.1.10" />
      <dependency id="Microsoft.Bcl.Build" version="1.0.21" />
      <dependency id="Microsoft.Net.Http" version="2.2.29" />
      <dependency id="SQLite.Net-PCL" version="3.1.1" />
    </dependencies>
  </metadata>
  <files>
